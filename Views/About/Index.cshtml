@using EntityLayer.Concrete
@model List<About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımda sayfası</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>Hakkımda başlığı</th>
        <th>Aktif yap</th>
        <th>Pasif yap</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>Başlık</td>
            <td><a href="#" class="btn btn-success">Aktif Yap</a></td>
            <td><a href="#" class="btn btn-danger">Pasif Yap</a></td>
        </tr>
    }
</table>



<br />

@Html.Action("AboutPartial","About")

